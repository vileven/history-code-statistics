{
    "compilerOptions": {
        "module": "commonjs",
        "baseUrl": "src",
        "lib": ["ES2021"],
        "target": "ES2022",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
}
